	#include<stdio.h>
	#include<string.h>
	
	void createMatch(char *s);
	int search(char *s,char *p);
	int match[500];
	
	int main(){
	    char simple[] = "haaadsafewnruzcewaradsfadsfiowe";
	    char pattern[] = "warads";
	    int position;
	    createMatch(simple);
	    position = search(simple,pattern);
	    
	    prinf("%d",position);
	    
	}
	
	int search(char *s,char *p){
	    int slen = strlen(s);
	    int plen = strlen(p);
	
	    int i,j;
	    i=j=0;
	    for(i=0;i<slen;i++){
	        if(s[i]==p[j]){
	            j++;
	        }else{
	            i--;
	            j = match[j]+1;
	        }
	
	        if(j==slen){
	            return i-slen;
	        }
	    }<++>
	}<++>
	
	
	void createMatch(char *s){
	    int len = strlen(s);
	    for(int i=0;i<len;i++){
	        s[i] = -1;
	    }
	
	    int p=0;
	    match[0]=-1;
	    for(int j=1;j<len;j++){
	
	        if(s[j]==s[p]){
	            match[j] = match[p];
	            p++;
	        }else{
	            while(match[p]!=0&&s[j]!=s[p]){
	                p = match[p];
	            }
	            match[j] = match[p];
	        }
	
	
	    }<++>
	
	}<++>
	
	
	
	
