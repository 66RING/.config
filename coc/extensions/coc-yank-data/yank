	    //             for( j=0; j<n;j++){
	    //                 if( set[temp][j] && !ar[j]){
	    //                     ar[j] = 1;
	    //                     q[++rear] = j;
	    //                 }
	    //             }
	    //         }
	    //         printf("}\n");
	    //     }
	    // }
	    for(int i=0;i<n;i++){
	        if(!ar[i]){
	            queue[++rear] = i;
	            ar[i] = 1;
	            printf("{ ");
	            while(front<rear){
	                printf("%d ",queue[++front]);
	                for(int j=queue[front];j<n;j++){
	                    if(!ar[j]&&set[queue[front]][j]){
	                        ar[j] = 1;
	                        queue[++rear] = j;
	                    }
	                }
	            }
	            printf("}\n");
	        }
	
	    }  
	    
	}
	
812f299e57b2e97a1ab3e3e6c9ef2218|file:///Users/ring/Documents/code/c/pintia_trains/teat.cpp|1|1|V|cpp
	#include<stdio.h>
	#include<stdlib.h>
	
	void build();
	void dfs(int n);
	void bfs(int n);
	
	int set[15][15] = {0};
	int visited[15] = {0};
	
	
	#define MAXVEX 15
	
	int Nv,Ne;
	
	int main(){
	    int n,e;
	    // scanf("%d %d",&n,&e);
	    build();
	    dfs(n);
	    bfs(n);
	    
	
	    return 0;
	}
	
	
	void build(){
	    //ç”¨é‚»æŽ¥çŸ©é˜µè¡¨ç¤ºå›¾
	    int i,j;
	    int v1,v2;
	    scanf("%d %d",&Nv,&Ne);
	    for( i=0; i<Nv; i++)
	    {
	        for( j=0; j<Nv; j++)
	        {
	            set[i][j] = 0;  //åˆå§‹åŒ–
	        }
	    }
	    for( i=0; i<Ne; i++)  //æ³¨æ„è¿™é‡Œæ˜¯è¯»å…¥è¾¹
	    {
	        scanf("%d %d",&v1,&v2);
	        set[v1][v2] = 1;
	        set[v2][v1]= set[v1][v2];  //æ— å‘å›¾å¯¹ç§°
	    }
	}
	
	
	
	
	void dfsP(int i,int n){
	    visited[i] = 1;
	    printf("%d ",i);
	    for(int j=i;j<n;j++){
	        if(!visited[j]&&set[i][j]){
	            dfsP(j,n);
	        }
	    }
	}
	
	void dfs(int n){
	    for(int i=0;i<n;i++){
	        if(!visited[i]){
	            printf("{ ");
	            dfsP(i,n);
	            printf("}\n");
	        }
	    }
	}
	
	
	void bfs(int n){
	    int *ar = (int*)malloc(sizeof(int)*n);
	    int font=-1,rear=-1;
	    int queue[11] = {0};
	
	    for(int i=0;i<n;i++){
	        ar[i]=0;
	    }
	
	    for(int i=0;i<n;i++){
	        if(!ar[i]){
	            queue[++rear] = i;
	            ar[i] = 1;
	            printf("{ ");
	            while(font<rear){
	                printf("%d ",queue[++font]);
	                for(int j=queue[font];j<n;j++){
	                    if(!ar[j]&&set[queue[font]][j]){
	                        ar[j] = 1;
	                        queue[++rear] = j;
	                    }
	                }
	            }
	            printf("}\n");
	        }
	
	    }  
	    
	}
	
