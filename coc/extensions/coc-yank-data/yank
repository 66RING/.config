	    //             printf("%d ",temp);
	    //             for( j=0; j<n;j++){
	    //                 if( set[temp][j] && !ar[j]){
	    //                     ar[j] = 1;
	    //                     q[++rear] = j;
	    //                 }
	    //             }
	    //         }
	    //         printf("}\n");
	    //     }
	    // }
	    for(int i=0;i<n;i++){
	        if(!ar[i]){
	            queue[++rear] = i;
	            ar[i] = 1;
	            printf("{ ");
	            while(front<rear){
	                printf("%d ",queue[++front]);
	                for(int j=queue[front];j<n;j++){
	                    if(!ar[j]&&set[queue[front]][j]){
	                        ar[j] = 1;
	                        queue[++rear] = j;
	                    }
	                }
	            }
	            printf("}\n");
	        }
	
	    }  
	    
	}
	
