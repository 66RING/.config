f783b93d8fd6185142e969ccfdb5318f|file:///Users/ring/Documents/code/c/pintia_trains/7-10_road_to_village.cpp|1|1|V|cpp
	#include<stdio.h>
	
	
	int set[101][101];
	
	void creat(int n,int m);
	void select(int n);
	
	
	int main(){
	    int n,m;
	    scanf("%d %d",&n,&m);
	    creat(n,m);
	    select(n);
	
	    return 0;
	}
	
	
	void creat(int n,int m){
	    int i,j,a;
	
	    for(int i=1;i<=n;i++){
	        for(int j=1;j<=n;j++){
	            set[i][j]=9999;
	        }
	    }
	    while(m--){
	        scanf("%d %d %d",&i,&j,&a);
	        set[i][j]=set[j][i]=a;
	    }
	}
	
	
	void select(int n){
	    for(int k=1;k<=n;k++){
	        for(int i=1;i<=n;i++){
	            for(int j=1;j<=n;j++){
	                if(i==j)set[i][j] = 0 ;
	                else if(i!=j&&set[i][j]>set[i][k]+set[k][j]){
	                    set[i][j] = set[i][k]+set[k][j];
	                }
	            }
	        }
	    }
	
	    int max[101]={0};
	    int min,tar;
	    int cnt = 0;
	    for(int i=1;i<=n;i++){
	        for(int j=1;j<=n;j++){
	            printf("%d ",set[i][j]);
	            max[i] += set[i][j];
	        }
	        printf(" \n");
	        if(max[i]==9999) cnt++;
	    }
	
	    if(cnt!=n){
	        min = max[1];tar=1;
	        for(int i=1;i<=n;i++){
	            if(max[i]<min){
	                min = max[i];
	                tar = i;
	            }
	        }
	
	        printf("%d %d",tar,max[tar]);
	    }else printf("1");
	}
	
