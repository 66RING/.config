	void createTree(int n,int m){
	    int max = 0;
	    for(int i=1;i<=n;i++){
	        action[i].early = 0;
	        action[i].last = 9999;
	    }
	    int a,b,t;
	    int i=0;
	    while(i<m){
	        scanf("%d %d %d",&a,&b,&t);
	        tree[a][b] = t;
	        end[a] = 0;
	        end[b] = 1;
	        out[a]++;
