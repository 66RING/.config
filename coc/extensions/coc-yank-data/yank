	#include<stdio.h>
	#include<stdlib.h>
	
	typedef struct{
	    int early,last;
	}Action;
	
	void createTree(int n,int m);
	void scan(int n);
	
	int end[200] = {0};
	int out[200] = {0};
	int in[200] = {0};
	int tree[200][200] = {0};  // 0 disconnect  1 indegree  2 outdegree
	Action action[200];
	int isG = 0;
	
	int main(){
	    int n,m;
	    scanf("%d %d",&n,&m);
	    createTree(n,m);
	    scan(n);
	
	//    for(int i=1;i<=n;i++){
	//        printf("%d:",end[i]);
	//        for(int j=1;j<=n;j++){
	//
	//            printf("%d ",tree[i][j]);
	//        }
	//        printf("\n");
	//    }
